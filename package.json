{
    "name": "messy",
    "version": "6.11.0",
    "description": "Object model for HTTP and RFC822 messages",
    "main": "lib/index.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "lint": "jshint .",
        "test": "mocha",
        "travis": "npm test && npm run coverage && (<coverage/lcov.info coveralls || true)",
        "coverage": "NODE_ENV=development istanbul cover _mocha -- --reporter dot && echo google-chrome coverage/lcov-report/index.html",
        "compile-lib": "babel lib --out-dir lib --require babel-polyfill --presets env",
        "compile-test": "babel test --out-dir test --require babel-polyfill --presets env",
        "compile-1_script": "npm run compile-lib && npm run compile-test",
        "compile": "npm run compile-1_script"
    },
    "keywords": [
        "http",
        "request",
        "response",
        "email",
        "message",
        "rfc822",
        "rfc2822"
    ],
    "author": "Andreas Lind <andreas@one.com>",
    "repository": "git://github.com/papandreou/messy.git",
    "license": "BSD",
    "devDependencies": {
        "coveralls": "2.11.2",
        "istanbul": "0.3.15",
        "jshint": "2.8.0",
        "mocha": "2.2.5",
        "unexpected": "8.0.1",
        "unexpected-messy": "^9.0.0",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-plugin-transform-object-rest-spread": "latest",
        "babel-preset-env": "latest"
    },
    "dependencies": {
        "iconv-lite": "^0.4.13",
        "quoted-printable": "1.0.0",
        "rfc2047": "2.0.0",
        "rfc2231": "1.3.0",
        "underscore": "^1.6.0"
    }
}
